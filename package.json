{
  "name": "testgraph",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon app.js",
    "compose": "docker-compose up --build -d",
    "jump": "docker exec -it graphql-app bash",
    "migrate": "sequelize db:migrate",
    "down": "docker-compose down && docker volume rm $(docker volume ls) || docker rmi $(docker images)"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "^4.16.4",
    "express-graphql": "^0.11.0",
    "graphql": "^15.4.0",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.7",
    "pg": "^8.4.2",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0"
  }
}
